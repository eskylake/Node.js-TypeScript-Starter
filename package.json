{
  "name": "test",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">= 20"
  },
  "devDependencies": {
    "@types/node": "~20",
    "@typescript-eslint/eslint-plugin": "~6",
    "@typescript-eslint/parser": "~6",
    "@vitest/coverage-v8": "^1",
    "eslint": "~8",
    "eslint-config-prettier": "~9",
    "prettier": "~3",
    "rimraf": "~5",
    "ts-api-utils": "~1",
    "typescript": "~5",
    "vitest": "^1"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp coverage.js coverage.cjs",
    "lint": "eslint . --ext .ts --ext .mts",
    "prebuild": "npm run lint",
    "prettier": "prettier --config .prettierrc --write .",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "coverage": "tsc coverage.ts && mv coverage.js coverage.cjs && node coverage.cjs",
    "test:coverage": "vitest run --coverage && npm run coverage",
    "test:watch": "vitest",
    "test": "vitest run --coverage",
    "ci": "npm run clean && npm run lint && npm run test:coverage && npm run build:release",
    "start": "node --enable-source-maps build/src/main.js"
  },
  "author": "Mohammad Hossein Fattahizadeh <m@mhf.ir>",
  "license": "MIT",
  "dependencies": {
    "tslib": "~2"
  }
}